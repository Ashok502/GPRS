<div class="col-sm-3">
	<div class="panel panel-default text-center">
		<div class="panel-body">
			<%= image_tag google_map(center: [ current_user.latitude, current_user.longitude ].join(',')) %>
		</div>
	</div>
	<div class="panel panel-default text-left">
		<div class="panel-heading">
			Users
		</div>
		<div class="panel-body">		
			<div class="user-friend-list">
				<%= render partial: '/profiles/user' %>
			</div>
		</div> 
	</div>
	<div id="ads">
		<%= render partial: "/ads/index" %>
	</div>	
</div>
<script>
	$(function(){
		$(".datepicker").datepicker({
			dateFormat: "yy-mm-dd"
		});
	})
</script>
